		<band height="861">
			<printWhenExpression><![CDATA[$P{CREDITO_PAGARE} == "TRUE"]]></printWhenExpression>
			<break>
				<reportElement x="0" y="810" width="100" height="1" uuid="0b278d99-21bc-4fc2-9bf0-34e65b742838"/>
